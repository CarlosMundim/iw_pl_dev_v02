# Mobile App Environment Configuration
# Copy this file to .env and fill in your actual values

# ============================================
# APPLICATION CONFIGURATION
# ============================================
# Environment
NODE_ENV=development
EXPO_ENV=development

# App Configuration
APP_NAME=iWORKZ
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1
BUNDLE_IDENTIFIER=jp.iworkz.app

# App Store Configuration
APPLE_APP_ID=123456789
GOOGLE_PLAY_APP_ID=jp.iworkz.app

# ============================================
# API CONFIGURATION
# ============================================
# Backend API URLs
API_BASE_URL=http://localhost:3000/api/v1
API_TIMEOUT=30000

# Environment-specific API URLs
DEV_API_URL=http://localhost:3000/api/v1
STAGING_API_URL=https://staging-api.iworkz.jp/api/v1
PRODUCTION_API_URL=https://api.iworkz.jp/api/v1

# WebSocket Configuration
WEBSOCKET_URL=ws://localhost:3001
WEBSOCKET_RECONNECT_INTERVAL=5000
WEBSOCKET_MAX_RECONNECT_ATTEMPTS=5

# ============================================
# AUTHENTICATION CONFIGURATION
# ============================================
# OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here.apps.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id_here.apps.googleusercontent.com

# Apple Sign In
APPLE_SIGN_IN_ENABLED=true

# Facebook Configuration
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_NAME=iWORKZ

# LinkedIn Configuration
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here

# ============================================
# PUSH NOTIFICATIONS
# ============================================
# Firebase Configuration
FIREBASE_PROJECT_ID=iworkz-mobile-app
FIREBASE_APP_ID_IOS=1:123456789:ios:abcdef123456
FIREBASE_APP_ID_ANDROID=1:123456789:android:abcdef123456
FIREBASE_API_KEY_IOS=your_firebase_ios_api_key_here
FIREBASE_API_KEY_ANDROID=your_firebase_android_api_key_here
FIREBASE_MESSAGING_SENDER_ID=123456789

# APNs Configuration (iOS)
APNS_KEY_ID=your_apns_key_id_here
APNS_TEAM_ID=your_apple_team_id_here

# ============================================
# ANALYTICS CONFIGURATION
# ============================================
# Google Analytics
GOOGLE_ANALYTICS_TRACKING_ID=GA-XXXX-X

# Firebase Analytics
FIREBASE_ANALYTICS_ENABLED=true

# Amplitude Analytics
AMPLITUDE_API_KEY=your_amplitude_api_key_here

# Mixpanel Analytics
MIXPANEL_TOKEN=your_mixpanel_token_here

# ============================================
# MAPS AND LOCATION
# ============================================
# Google Maps Configuration
GOOGLE_MAPS_API_KEY_IOS=your_google_maps_ios_api_key_here
GOOGLE_MAPS_API_KEY_ANDROID=your_google_maps_android_api_key_here

# Apple Maps Configuration
APPLE_MAPS_ENABLED=true

# Location Services
LOCATION_ACCURACY=high
LOCATION_TIMEOUT=15000

# ============================================
# STORAGE CONFIGURATION
# ============================================
# AsyncStorage Configuration
ASYNC_STORAGE_ENCRYPTION_ENABLED=true
ASYNC_STORAGE_KEY_PREFIX=iworkz_

# Secure Storage
KEYCHAIN_SERVICE=jp.iworkz.app
KEYCHAIN_ACCESS_GROUP=group.jp.iworkz.app

# ============================================
# CAMERA AND MEDIA
# ============================================
# Camera Configuration
CAMERA_ENABLED=true
CAMERA_QUALITY=0.8
IMAGE_MAX_WIDTH=1024
IMAGE_MAX_HEIGHT=1024

# Document Scanner
DOCUMENT_SCANNER_ENABLED=true
OCR_ENABLED=true

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,doc,docx,jpg,jpeg,png

# ============================================
# DEEP LINKING
# ============================================
# URL Scheme
URL_SCHEME=iworkz
UNIVERSAL_LINK_DOMAIN=app.iworkz.jp

# Branch.io Configuration
BRANCH_KEY=key_live_your_branch_key_here
BRANCH_TEST_KEY=key_test_your_branch_test_key_here

# ============================================
# ERROR REPORTING
# ============================================
# Sentry Configuration
SENTRY_DSN=https://your_sentry_dsn_here@sentry.io/project_id
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=1.0.0

# Bugsnag Configuration
BUGSNAG_API_KEY=your_bugsnag_api_key_here

# ============================================
# APP STORE CONFIGURATION
# ============================================
# Over-the-Air Updates
OTA_UPDATES_ENABLED=true
CODEPUSH_KEY_IOS=your_codepush_ios_key_here
CODEPUSH_KEY_ANDROID=your_codepush_android_key_here

# App Store Connect
APP_STORE_CONNECT_KEY_ID=your_app_store_connect_key_id
APP_STORE_CONNECT_ISSUER_ID=your_app_store_connect_issuer_id

# ============================================
# BIOMETRIC AUTHENTICATION
# ============================================
# Biometric Settings
BIOMETRIC_ENABLED=true
FACE_ID_ENABLED=true
TOUCH_ID_ENABLED=true
FINGERPRINT_ENABLED=true

# ============================================
# OFFLINE CAPABILITIES
# ============================================
# Offline Storage
OFFLINE_STORAGE_ENABLED=true
OFFLINE_STORAGE_SIZE_LIMIT=50MB
SYNC_INTERVAL=300000

# Cache Configuration
CACHE_ENABLED=true
CACHE_SIZE_LIMIT=100MB
IMAGE_CACHE_SIZE_LIMIT=50MB

# ============================================
# INTERNATIONALIZATION
# ============================================
# Language Settings
DEFAULT_LANGUAGE=ja
SUPPORTED_LANGUAGES=en,ja,ko,zh
RTL_SUPPORT_ENABLED=false

# ============================================
# PERFORMANCE MONITORING
# ============================================
# Performance Monitoring
PERFORMANCE_MONITORING_ENABLED=true
CRASH_REPORTING_ENABLED=true

# React Native Performance
FLIPPER_ENABLED=true
HERMES_ENABLED=true
NEW_ARCHITECTURE_ENABLED=false

# ============================================
# SECURITY CONFIGURATION
# ============================================
# App Security
ROOT_DETECTION_ENABLED=true
JAILBREAK_DETECTION_ENABLED=true
DEBUG_DETECTION_ENABLED=true

# Certificate Pinning
SSL_PINNING_ENABLED=true
CERTIFICATE_HASHES=sha256/your_certificate_hash_here

# ============================================
# VOICE ASSISTANT INTEGRATION
# ============================================
# Voice Recognition
VOICE_RECOGNITION_ENABLED=true
SPEECH_TO_TEXT_LANGUAGE=ja-JP
TEXT_TO_SPEECH_ENABLED=true

# AI Assistant
AI_ASSISTANT_ENABLED=true
VOICE_COMMANDS_ENABLED=true

# ============================================
# QR CODE AND BARCODE
# ============================================
# QR Code Scanner
QR_CODE_SCANNER_ENABLED=true
BARCODE_SCANNER_ENABLED=true

# Business Card Scanner
BUSINESS_CARD_SCANNER_ENABLED=true
OCR_LANGUAGE=ja

# ============================================
# ACCESSIBILITY
# ============================================
# Accessibility Features
ACCESSIBILITY_ENABLED=true
SCREEN_READER_SUPPORT=true
HIGH_CONTRAST_MODE=true
LARGE_TEXT_SUPPORT=true

# ============================================
# DEVELOPMENT CONFIGURATION
# ============================================
# Debug Settings
DEBUG_MODE=true
CONSOLE_LOGS_ENABLED=true
NETWORK_LOGGING_ENABLED=true

# Development Tools
FLIPPER_ENABLED=true
REACTOTRON_ENABLED=true
STORYBOOK_ENABLED=false

# ============================================
# TESTING CONFIGURATION
# ============================================
# Testing Environment
TEST_MODE=false
MOCK_API_ENABLED=false
E2E_TESTING_ENABLED=false

# Test Data
TEST_USER_EMAIL=test@iworkz.jp
TEST_USER_PASSWORD=testpassword123

# ============================================
# BUILD CONFIGURATION
# ============================================
# Expo Configuration
EXPO_PUBLIC_API_URL=https://api.iworkz.jp
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_public_google_maps_key

# Metro Configuration
METRO_PORT=8081
METRO_HOST=localhost

# ============================================
# FEATURE FLAGS
# ============================================
# Feature Toggles
FEATURE_DARK_MODE=true
FEATURE_VOICE_ASSISTANT=true
FEATURE_BLOCKCHAIN_VERIFICATION=true
FEATURE_AI_RECOMMENDATIONS=true
FEATURE_VIDEO_INTERVIEWS=false
FEATURE_CHAT_SUPPORT=true
FEATURE_OFFLINE_MODE=true

# ============================================
# COMPLIANCE CONFIGURATION
# ============================================
# Privacy Settings
PRIVACY_POLICY_URL=https://iworkz.jp/privacy
TERMS_OF_SERVICE_URL=https://iworkz.jp/terms
GDPR_COMPLIANCE_ENABLED=true

# Japanese Compliance
JAPANESE_EMPLOYMENT_LAW_ENABLED=true
PERSONAL_INFO_PROTECTION_ENABLED=true

# ============================================
# SOCIAL SHARING
# ============================================
# Social Media Integration
TWITTER_ENABLED=true
FACEBOOK_SHARING_ENABLED=true
LINKEDIN_SHARING_ENABLED=true
LINE_SHARING_ENABLED=true

# ============================================
# PRODUCTION OVERRIDES
# ============================================
# These variables should be set in production
# NODE_ENV=production
# API_BASE_URL=https://api.iworkz.jp/api/v1
# SENTRY_DSN=production_sentry_dsn
# GOOGLE_ANALYTICS_TRACKING_ID=production_ga_id
# FIREBASE_PROJECT_ID=production_project_id