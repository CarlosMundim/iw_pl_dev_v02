      "role": "AI-Integration Architecture Leader & Strategic Advisor",
      "linkedin": "https://www.linkedin.com/in/jeanette-dennisson-246063a1/"
    },
    {
      "name": "Claude Code (Anthropic)",
      "role": "AI Development Partner & Documentation Architect",
      "contributions": "Frontend architecture, React development, UI/UX implementation"
    }
  ],
  "technologies": [
    "React",
    "Next.js 14",
    "TypeScript",
    "Tailwind CSS",
    "Zustand"
  ],
  "platforms": [
    "Vercel",
    "Ubuntu",
    "WSL2"
  ],
  "tools": [
    "GitHub",
    "Git",
    "Visual Studio Code",
    "npm"
  ],
  "aiTools": [
    "Claude Code",
    "GitHub Copilot"
  ],
  "handoverDate": "2025-06-08",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:components": "jest tests/components --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:performance": "jest tests/performance --coverage",
    "test:cultural": "jest tests/components/CulturalComponents.test.tsx --coverage",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:quick": "jest --onlyChanged --passWithNoTests",
    "test:related": "jest --findRelatedTests",
    "test:changed": "jest --findRelatedTests --passWithNoTests",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:build": "npm run build && npm run test:smoke",
    "load:smoke": "k6 run --env ENV=local tests/performance/k6/smoke-test.js",
    "load:stress": "k6 run --env ENV=staging tests/performance/k6/stress-test.js",
    "load:spike": "k6 run --env ENV=staging tests/performance/k6/spike-test.js",
    "load:endurance": "k6 run --env ENV=production tests/performance/k6/endurance-test.js",
    "load:cultural": "k6 run --env ENV=staging tests/performance/k6/cultural-intelligence-test.js",
    "load:api": "k6 run --env ENV=staging tests/performance/k6/api-load-test.js",
    "lighthouse:audit": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "performance:analyze": "node scripts/performance-analysis.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "validate:cultural-accuracy": "node scripts/cultural-validation.js",
    "test:japan-features": "jest --testPathPattern=japan",
    "test:business-etiquette": "jest --testPathPattern=etiquette",
    "detect:cultural-bias": "node scripts/cultural-bias-detection.js",
    "report:cultural-validation": "node scripts/cultural-report.js",
    "generate:performance-report": "node scripts/generate-performance-report.js",
    "upload:performance-data": "node scripts/upload-performance-data.js",
    "monitor:post-deployment": "node scripts/post-deployment-monitor.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.295.0",
    "next": "14.0.4",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.2",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-query": "^3.39.3",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
